var obj = {a:1,b:2, c:3}
for(let prop in obj){
    console.log("obj", prop, "=",obj[prop])
}
var iteravel=[{nome:"Danilo", prof:true},
    {nome:"Sergio", prof:true},
    {nome:"Ramiro", prof:false}]
for (const valor of iteravel){
    console.log(valor.nome)
}
var interavel="casa"
